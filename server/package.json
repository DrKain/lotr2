{
  "name": "lotr-server",
  "version": "1.0.0",
  "description": "Backend of Land of the Rair",
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup",
    "start:dev": "nodemon --ignore \"dist/*\" --ignore \"*.json\" --exec \"ts-node \" -- \"src/core/index.ts\"",
    "start:dev:single": "nodemon --ignore \"dist/*\" --ignore \"*.json\" --exec \"ts-node \" -- \"src/core/index.ts\" --single-core",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc"
  },
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/landoftherair/core2.git"
  },
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/landoftherair/core2/issues"
  },
  "homepage": "https://github.com/landoftherair/core2#readme",
  "dependencies": {
    "@clusterws/cws": "^0.15.2",
    "censor-sensor": "^1.0.5",
    "dotenv": "^8.1.0",
    "download-github-repo": "^0.1.4",
    "fast-json-patch": "^3.0.0-1",
    "fastify": "^2.9.0",
    "fastify-rate-limit": "^2.4.0",
    "l1-path-finder": "^1.0.0",
    "mongodb": "^3.3.2",
    "mrpas": "^2.0.0",
    "nodemon": "^1.19.3",
    "notepack.io": "^2.2.0",
    "process-stats": "^3.1.1",
    "rbush": "^3.0.1",
    "recursive-readdir": "^2.2.2",
    "ts-node": "^8.4.1",
    "typescript-ioc": "^1.2.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.11",
    "codelyzer": "^5.1.2",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0-beta"
  }
}
